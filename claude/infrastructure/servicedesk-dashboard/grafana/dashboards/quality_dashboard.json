      {
        "id": 6,
        "type": "stat",
        "title": "Quality Analysis Coverage",
        "description": "% of comments analyzed for quality",
        "gridPos": {
          "h": 4,
          "w": 4,
          "x": 22,
          "y": 0
        },
        "targets": [
          {
            "datasource": {
              "type": "postgres",
              "uid": "P6BECECF7273D15EE"
            },
            "rawSql": "SELECT ROUND(100.0 * COUNT(DISTINCT cq.comment_id) / (SELECT COUNT(*) FROM servicedesk.comments), 2) as \"Coverage %\" FROM servicedesk.comment_quality cq;",
            "refId": "A",
            "format": "table"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "thresholds": {
              "mode": "absolute",
              "steps": [
