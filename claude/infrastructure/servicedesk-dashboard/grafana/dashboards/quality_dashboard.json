          "x": 12,
          "y": 8
        },
        "targets": [
          {
            "datasource": {
              "type": "postgres",
              "uid": "P6BECECF7273D15EE"
            },
            "rawSql": "SELECT 'Professionalism' as dimension, ROUND(AVG(professionalism_score)::numeric, 2) as score FROM servicedesk.comment_quality UNION ALL SELECT 'Clarity', ROUND(AVG(clarity_score)::numeric, 2) FROM servicedesk.comment_quality UNION ALL SELECT 'Empathy', ROUND(AVG(empathy_score)::numeric, 2) FROM servicedesk.comment_quality UNION ALL SELECT 'Actionability', ROUND(AVG(actionability_score)::numeric, 2) FROM servicedesk.comment_quality;",
            "refId": "A",
            "format": "table"
          }
        ],
        "options": {
