    "editable": true,
    "fiscalYearStartMonth": 0,
    "graphTooltip": 1,
    "links": [],
    "liveNow": false,
    "panels": [
      {
        "id": 1,
        "type": "stat",
        "title": "Reassignment Rate",
        "description": "Percentage of tickets requiring reassignment (multiple agents)\nTarget: \u226420% (Green)\nAcceptable: 21-30% (Yellow)\nCritical: >30% (Red)\n\n\u26a0\ufe0f Coverage: 9.6% (762/7,969 tickets with timesheet data)",
        "gridPos": {
          "h": 6,
          "w": 4,
          "x": 0,
          "y": 0
        },
        "targets": [
          {
            "datasource": {
              "type": "postgres",
              "uid": "${datasource}"
            },
            "format": "table",
            "rawSql": "WITH ticket_agent_counts AS (\n  SELECT\n    \"TS-Ticket Project Master Code\" as ticket_id,\n    COUNT(DISTINCT \"TS-User Full Name\") as agent_count\n  FROM servicedesk.timesheets\n  WHERE \"TS-Ticket Project Master Code\" IS NOT NULL\n  GROUP BY \"TS-Ticket Project Master Code\"\n)\nSELECT\n  ROUND(100.0 * SUM(CASE WHEN agent_count > 1 THEN 1 ELSE 0 END) / COUNT(*), 2) as \"Reassignment %\"\nFROM ticket_agent_counts;",
            "refId": "A"
          }
        ],
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": [
              "lastNotNull"
            ]
          },
          "orientation": "auto",
          "textMode": "value_and_name",
          "colorMode": "background",
          "graphMode": "none",
          "justifyMode": "auto"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "value": null,
                  "color": "green"
                },
                {
                  "value": 21,
                  "color": "yellow"
                },
                {
                  "value": 31,
                  "color": "red"
                }
              ]
            },
            "decimals": 2
          }
        }
      },
      {
        "id": 2,
        "type": "stat",
        "title": "Total Ticket Volume (90d)",
        "description": "Total tickets created in last 90 days",
        "gridPos": {
          "h": 6,
          "w": 4,
          "x": 4,
          "y": 0
        },
        "targets": [
          {
            "datasource": {
              "type": "postgres",
              "uid": "${datasource}"
            },
            "format": "table",
            "rawSql": "SELECT COUNT(*) as \"Total Tickets\"\nFROM servicedesk.tickets\nWHERE \"TKT-Team\" IN ('${team:raw}') AND \"TKT-Created Time\" >= NOW() - INTERVAL '90 days';",
            "refId": "A"
          }
        ],
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": [
              "lastNotNull"
            ]
          },
          "orientation": "auto",
          "textMode": "value_and_name",
          "colorMode": "value",
          "graphMode": "area",
          "justifyMode": "auto"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "value": null,
                  "color": "blue"
                }
              ]
            },
            "decimals": 0
          }
        }
      },
      {
        "id": 3,
        "type": "barchart",
        "title": "Top 10 Root Cause Categories",
        "description": "Distribution of tickets by root cause category",
        "gridPos": {
          "h": 8,
          "w": 8,
          "x": 8,
          "y": 0
        },
        "targets": [
          {
            "datasource": {
              "type": "postgres",
              "uid": "${datasource}"
            },
            "format": "table",
            "rawSql": "SELECT\n  \"TKT-Root Cause Category\" as \"Root Cause\",\n  COUNT(*) as \"Ticket Count\"\nFROM servicedesk.tickets\nWHERE \"TKT-Team\" IN ('${team:raw}') AND \"TKT-Status\" IN ('Closed', 'Resolved')\n  AND \"TKT-Root Cause Category\" IS NOT NULL\n  AND \"TKT-Root Cause Category\" != ''\nGROUP BY \"TKT-Root Cause Category\"\nORDER BY COUNT(*) DESC\nLIMIT 10;",
            "refId": "A"
          }
        ],
        "options": {
          "orientation": "horizontal",
          "xTickLabelRotation": 0,
          "xTickLabelSpacing": 100,
          "showValue": "always",
          "stacking": "none",
          "groupWidth": 0.7,
          "barWidth": 0.97,
          "fullHighlight": false,
          "legend": {
            "showLegend": false,
            "displayMode": "list",
            "placement": "bottom",
            "calcs": []
          }
        },
        "fieldConfig": {
          "defaults": {
            "custom": {
              "axisPlacement": "auto",
              "barAlignment": 0,
              "drawStyle": "line",
              "fillOpacity": 80,
              "gradientMode": "none",
              "hideFrom": {
                "tooltip": false,
                "viz": false,
                "legend": false
              },
              "lineInterpolation": "linear",
              "lineWidth": 1,
              "pointSize": 5,
              "scaleDistribution": {
                "type": "linear"
              },
              "showPoints": "auto",
              "spanNulls": false,
              "stacking": {
                "group": "A",
                "mode": "none"
              },
              "thresholdsStyle": {
                "mode": "off"
              }
            },
            "color": {
              "mode": "palette-classic"
            },
            "unit": "short"
          }
        }
      },
      {
        "id": 4,
        "type": "piechart",
        "title": "Team Workload Distribution",
        "description": "Ticket volume by team (Top 10)",
        "gridPos": {
          "h": 8,
          "w": 8,
          "x": 16,
          "y": 0
        },
        "targets": [
          {
            "datasource": {
              "type": "postgres",
              "uid": "${datasource}"
            },
            "format": "table",
            "rawSql": "SELECT\n  \"TKT-Team\" as \"Team\",\n  COUNT(*) as \"Tickets\"\nFROM servicedesk.tickets\nWHERE \"TKT-Status\" IN ('Closed', 'Resolved')\n  AND \"TKT-Team\" IS NOT NULL\nGROUP BY \"TKT-Team\"\nORDER BY COUNT(*) DESC\nLIMIT 10;",
            "refId": "A"
          }
        ],
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": [
              "lastNotNull"
            ]
          },
          "pieType": "pie",
          "tooltip": {
            "mode": "single",
            "sort": "none"
          },
          "legend": {
            "showLegend": true,
            "displayMode": "table",
            "placement": "right",
            "calcs": [
              "sum"
            ],
            "values": [
              "value"
            ]
          },
          "displayLabels": [
            "percent"
          ]
        },
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "decimals": 0
          }
        }
      },
      {
        "id": 5,
        "type": "timeseries",
        "title": "Monthly Ticket Volume Trend",
        "description": "Total tickets created per month",
        "gridPos": {
          "h": 8,
          "w": 16,
          "x": 0,
          "y": 8
        },
        "targets": [
          {
            "datasource": {
              "type": "postgres",
              "uid": "${datasource}"
            },
            "format": "time_series",
            "rawSql": "SELECT\n  DATE_TRUNC('month', \"TKT-Created Time\") as time,\n  COUNT(*) as \"Ticket Volume\"\nFROM servicedesk.tickets\nWHERE \"TKT-Team\" IN ('${team:raw}') AND \"TKT-Created Time\" >= NOW() - INTERVAL '90 days'\nGROUP BY DATE_TRUNC('month', \"TKT-Created Time\")\nORDER BY time;",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "drawStyle": "bars",
              "lineInterpolation": "linear",
              "barAlignment": 0,
              "lineWidth": 1,
              "fillOpacity": 80,
              "gradientMode": "none",
              "spanNulls": false,
              "showPoints": "auto",
              "pointSize": 5,
              "stacking": {
                "mode": "none",
                "group": "A"
              },
              "axisPlacement": "auto",
              "axisLabel": "",
              "scaleDistribution": {
                "type": "linear"
              }
            },
            "color": {
              "mode": "palette-classic"
            },
            "unit": "short",
            "decimals": 0
          }
        },
        "options": {
          "tooltip": {
            "mode": "single",
            "sort": "none"
          },
          "legend": {
            "showLegend": true,
            "displayMode": "list",
            "placement": "bottom",
            "calcs": []
          }
        }
      },
      {
        "id": 6,
        "type": "barchart",
        "title": "Resolution Time by Team (Days)",
        "description": "Average resolution time per team (teams with \u226550 tickets)",
        "gridPos": {
          "h": 8,
          "w": 8,
          "x": 16,
          "y": 8
        },
        "targets": [
          {
            "datasource": {
              "type": "postgres",
              "uid": "${datasource}"
            },
            "format": "table",
            "rawSql": "SELECT\n  \"TKT-Team\" as \"Team\",\n  ROUND(AVG(EXTRACT(EPOCH FROM (\"TKT-Actual Resolution Date\" - \"TKT-Created Time\")) / 86400)::numeric, 2) as \"Avg Days\"\nFROM servicedesk.tickets\nWHERE \"TKT-Status\" IN ('Closed', 'Resolved')\n  AND \"TKT-Actual Resolution Date\" IS NOT NULL\n  AND \"TKT-Team\" IS NOT NULL\nGROUP BY \"TKT-Team\"\nHAVING COUNT(*) >= 50\nORDER BY \"Avg Days\" ASC\nLIMIT 10;",
            "refId": "A"
          }
        ],
        "options": {
          "orientation": "horizontal",
          "xTickLabelRotation": 0,
          "showValue": "always",
          "stacking": "none",
          "groupWidth": 0.7,
          "barWidth": 0.97,
          "legend": {
            "showLegend": false
          }
        },
        "fieldConfig": {
          "defaults": {
            "custom": {
              "fillOpacity": 80
            },
            "color": {
              "mode": "thresholds"
            },
            "unit": "d",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "value": null,
                  "color": "green"
                },
                {
                  "value": 3,
                  "color": "yellow"
                },
                {
                  "value": 7,
                  "color": "red"
                }
              ]
            },
            "decimals": 2
          }
        }
      },
      {
        "id": 7,
        "type": "barchart",
        "title": "Team Customer Communication Coverage",
        "description": "% of tickets with customer-facing comments by team",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 16
        },
        "targets": [
          {
            "datasource": {
              "type": "postgres",
              "uid": "${datasource}"
            },
            "format": "table",
            "rawSql": "WITH customer_facing_comments AS (\n  SELECT ticket_id, COUNT(*) as customer_comment_count\n  FROM servicedesk.comments WHERE visible_to_customer = 'Yes' GROUP BY ticket_id\n)\nSELECT\n  t.\"TKT-Team\" as \"Team\",\n  ROUND(100.0 * SUM(CASE WHEN cfc.customer_comment_count > 0 THEN 1 ELSE 0 END) / COUNT(*), 2) as \"Coverage %\"\nFROM servicedesk.tickets t\nLEFT JOIN customer_facing_comments cfc ON t.\"TKT-Ticket ID\" = cfc.ticket_id\nWHERE t.\"TKT-Status\" IN ('Closed', 'Resolved')\nGROUP BY t.\"TKT-Team\"\nHAVING COUNT(*) >= 50\nORDER BY \"Coverage %\" DESC\nLIMIT 10;",
            "refId": "A"
          }
        ],
        "options": {
          "orientation": "horizontal",
          "showValue": "always",
          "stacking": "none",
          "legend": {
            "showLegend": false
          }
        },
        "fieldConfig": {
          "defaults": {
            "custom": {
              "fillOpacity": 80
            },
            "color": {
              "mode": "thresholds"
            },
            "unit": "percent",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "value": null,
                  "color": "red"
                },
                {
                  "value": 75,
                  "color": "yellow"
                },
                {
                  "value": 90,
                  "color": "green"
                }
              ]
            },
            "decimals": 2
          }
        }
      },
      {
        "id": 8,
        "type": "piechart",
        "title": "Quality Tier Distribution",
        "description": "Distribution of comment quality ratings\n\n\u26a0\ufe0f Coverage: 0.5% (517/108,129 comments)",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 16
        },
        "targets": [
          {
            "datasource": {
              "type": "postgres",
              "uid": "${datasource}"
            },
            "format": "table",
            "rawSql": "SELECT\n  quality_tier as \"Quality Tier\",\n  COUNT(*) as \"Comments\"\nFROM servicedesk.comment_quality\nWHERE quality_tier IS NOT NULL\nGROUP BY quality_tier\nORDER BY CASE quality_tier\n  WHEN 'excellent' THEN 1\n  WHEN 'good' THEN 2\n  WHEN 'acceptable' THEN 3\n  WHEN 'poor' THEN 4\nEND;",
            "refId": "A"
          }
        ],
        "options": {
          "pieType": "donut",
          "tooltip": {
            "mode": "single"
          },
          "legend": {
            "showLegend": true,
            "displayMode": "table",
            "placement": "right",
            "calcs": [
              "sum",
              "percent"
            ],
            "values": [
              "value",
              "percent"
            ]
          },
          "displayLabels": [
            "name",
            "percent"
          ]
        },
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "decimals": 0,
            "color": {
              "mode": "fixed",
              "fixedColor": "semi-dark-red"
            },
            "mappings": [
              {
                "type": "value",
                "value": "excellent",
                "text": "Excellent",
                "color": "dark-green"
              },
              {
                "type": "value",
                "value": "good",
                "text": "Good",
                "color": "green"
              },
              {
                "type": "value",
                "value": "acceptable",
                "text": "Acceptable",
                "color": "yellow"
              },
              {
                "type": "value",
                "value": "poor",
                "text": "Poor",
                "color": "red"
              }
            ]
          }
        }
      },
      {
        "id": 9,
        "type": "stat",
        "title": "SLA Risk Indicator",
        "description": "Open tickets grouped by age risk level\nSafe: < 3 days old\nWarning: 3-7 days old\nCritical: > 7 days old (SLA breach risk)",
        "gridPos": {
          "h": 4,
          "w": 8,
          "x": 0,
          "y": 24
        },
        "targets": [
          {
            "datasource": {
              "type": "postgres",
              "uid": "${datasource}"
            },
            "format": "table",
            "rawSql": "SELECT \n  COUNT(*) FILTER (WHERE \"TKT-Created Time\" >= NOW() - INTERVAL '3 days') as \"Safe (< 3d)\",\n  COUNT(*) FILTER (WHERE \"TKT-Created Time\" < NOW() - INTERVAL '3 days' \n                   AND \"TKT-Created Time\" >= NOW() - INTERVAL '7 days') as \"Warning (3-7d)\",\n  COUNT(*) FILTER (WHERE \"TKT-Created Time\" < NOW() - INTERVAL '7 days') as \"Critical (> 7d)\"\nFROM servicedesk.tickets\nWHERE \"TKT-Status\" NOT IN ('Closed', 'Incident Resolved');",
            "refId": "A"
          }
        ],
        "options": {
          "reduceOptions": {
            "values": true,
            "calcs": []
          },
          "orientation": "horizontal",
          "textMode": "value_and_name",
          "colorMode": "background",
          "graphMode": "none",
          "justifyMode": "auto"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "decimals": 0
          },
          "overrides": [
            {
              "matcher": {
                "id": "byName",
                "options": "Safe (< 3d)"
              },
              "properties": [
                {
                  "id": "color",
                  "value": {
                    "mode": "fixed",
                    "fixedColor": "green"
                  }
                }
              ]
            },
            {
              "matcher": {
                "id": "byName",
                "options": "Warning (3-7d)"
              },
              "properties": [
                {
                  "id": "color",
                  "value": {
                    "mode": "fixed",
                    "fixedColor": "yellow"
                  }
                }
              ]
            },
            {
              "matcher": {
                "id": "byName",
                "options": "Critical (> 7d)"
              },
              "properties": [
                {
                  "id": "color",
                  "value": {
                    "mode": "fixed",
                    "fixedColor": "red"
                  }
                }
              ]
            }
          ]
        }
      },
      {
        "id": 10,
        "type": "barchart",
        "title": "Open Ticket Age Distribution",
        "description": "Distribution of open tickets by age bucket\nCritical insight: 52% of open tickets are > 30 days old",
        "gridPos": {
          "h": 8,
          "w": 8,
          "x": 8,
          "y": 24
        },
        "targets": [
          {
            "datasource": {
              "type": "postgres",
              "uid": "${datasource}"
            },
            "format": "table",
            "rawSql": "WITH age_calc AS (\n  SELECT \n    EXTRACT(DAY FROM NOW() - \"TKT-Created Time\") as age_days\n  FROM servicedesk.tickets\n  WHERE \"TKT-Status\" NOT IN ('Closed', 'Incident Resolved')\n)\nSELECT \n  CASE \n    WHEN age_days < 1 THEN '1: < 1 day'\n    WHEN age_days < 7 THEN '2: 1-7 days'\n    WHEN age_days < 30 THEN '3: 1-4 weeks'\n    WHEN age_days < 90 THEN '4: 1-3 months'\n    WHEN age_days < 180 THEN '5: 3-6 months'\n    ELSE '6: > 6 months'\n  END as \"Age Bucket\",\n  COUNT(*) as \"Open Tickets\"\nFROM age_calc\nGROUP BY 1\nORDER BY 1;",
            "refId": "A"
          }
        ],
        "options": {
          "orientation": "horizontal",
          "xTickLabelRotation": 0,
          "showValue": "always",
          "stacking": "none",
          "groupWidth": 0.7,
          "barWidth": 0.97,
          "legend": {
            "showLegend": false
          }
        },
        "fieldConfig": {
          "defaults": {
            "custom": {
              "fillOpacity": 80
            },
            "color": {
              "mode": "thresholds"
            },
            "unit": "short",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "value": null,
                  "color": "green"
                },
                {
                  "value": 500,
                  "color": "yellow"
                },
                {
                  "value": 1000,
                  "color": "orange"
                },
                {
                  "value": 2000,
                  "color": "red"
                }
              ]
            },
            "decimals": 0
          }
        }
      },
      {
        "id": 11,
        "type": "barchart",
        "title": "Resolution Time Distribution",
        "description": "Distribution of closed tickets by resolution time\nCritical: 41.6% take > 7 days to resolve",
        "gridPos": {
          "h": 8,
          "w": 8,
          "x": 16,
          "y": 24
        },
        "targets": [
          {
            "datasource": {
              "type": "postgres",
              "uid": "${datasource}"
            },
            "format": "table",
            "rawSql": "SELECT \n  CASE \n    WHEN EXTRACT(EPOCH FROM (\"TKT-Modified Time\" - \"TKT-Created Time\")) / 3600 < 1 THEN '1: < 1 hour'\n    WHEN EXTRACT(EPOCH FROM (\"TKT-Modified Time\" - \"TKT-Created Time\")) / 3600 < 24 THEN '2: 1-24 hours'\n    WHEN EXTRACT(EPOCH FROM (\"TKT-Modified Time\" - \"TKT-Created Time\")) / 86400 < 3 THEN '3: 1-3 days'\n    WHEN EXTRACT(EPOCH FROM (\"TKT-Modified Time\" - \"TKT-Created Time\")) / 86400 < 7 THEN '4: 3-7 days'\n    ELSE '5: > 7 days'\n  END as \"Resolution Time\",\n  COUNT(*) as \"Tickets\"\nFROM servicedesk.tickets\nWHERE \"TKT-Status\" IN ('Closed', 'Incident Resolved')\n  AND \"TKT-Modified Time\" > \"TKT-Created Time\"\nGROUP BY 1\nORDER BY 1;",
            "refId": "A"
          }
        ],
        "options": {
          "orientation": "horizontal",
          "xTickLabelRotation": 0,
          "showValue": "always",
          "stacking": "none",
          "groupWidth": 0.7,
          "barWidth": 0.97,
          "legend": {
            "showLegend": false
          }
        },
        "fieldConfig": {
          "defaults": {
            "custom": {
              "fillOpacity": 80
            },
            "color": {
              "mode": "thresholds"
            },
            "unit": "short",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "value": null,
                  "color": "green"
                },
                {
                  "value": 20000,
                  "color": "yellow"
                },
                {
                  "value": 40000,
                  "color": "orange"
                },
                {
                  "value": 60000,
                  "color": "red"
                }
              ]
            },
            "decimals": 0
          }
        }
      },
      {
        "id": 12,
        "type": "table",
        "title": "Recurring Alert Patterns (Grouped)",
        "description": "Alert tickets grouped by pattern for root cause analysis\nTop patterns indicate automation opportunities",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 32
        },
        "targets": [
          {
            "datasource": {
              "type": "postgres",
              "uid": "${datasource}"
            },
            "format": "table",
            "rawSql": "SELECT \n  CASE \n    WHEN \"TKT-Title\" ILIKE '%azure%memory%' OR \"TKT-Title\" ILIKE '%50.000000 % of physical memory%' THEN 'Azure Memory Alerts'\n    WHEN \"TKT-Title\" ILIKE '%cusf%network%' THEN 'CUSF Network Alerts'\n    WHEN \"TKT-Title\" ILIKE '%whephdc1%' THEN 'whephdc1 VM Status Alerts'\n    WHEN \"TKT-Title\" ILIKE '%motion detected%' THEN 'Motion Detection Alerts'\n    WHEN \"TKT-Title\" ILIKE '%vpn connectivity%' THEN 'VPN Connectivity Alerts'\n    WHEN \"TKT-Title\" ILIKE '%volume latency%' THEN 'Volume Latency Alerts'\n    WHEN \"TKT-Title\" ILIKE '%disk%' THEN 'Disk Alerts'\n    ELSE 'Other Alerts'\n  END as \"Alert Pattern\",\n  COUNT(*) as \"Occurrences\",\n  COUNT(DISTINCT DATE_TRUNC('day', \"TKT-Created Time\")) as \"Days Affected\",\n  ROUND(COUNT(*)::numeric / NULLIF(COUNT(DISTINCT DATE_TRUNC('day', \"TKT-Created Time\")), 0), 1) as \"Avg/Day\"\nFROM servicedesk.tickets\nWHERE \"TKT-Category\" = 'Alert'\nGROUP BY 1\nORDER BY 2 DESC\nLIMIT 10;",
            "refId": "A"
          }
        ],
        "options": {
          "showHeader": true,
          "cellHeight": "sm",
          "footer": {
            "show": false
          }
        },
        "fieldConfig": {
          "defaults": {
            "custom": {
              "align": "auto",
              "displayMode": "auto"
            }
          },
          "overrides": [
            {
              "matcher": {
                "id": "byName",
                "options": "Occurrences"
              },
              "properties": [
                {
                  "id": "custom.displayMode",
                  "value": "gradient-gauge"
                },
                {
                  "id": "max",
                  "value": 20000
                },
                {
                  "id": "thresholds",
                  "value": {
                    "mode": "absolute",
