            "format": "table",
            "rawSql": "WITH team_stats AS (\n  SELECT \n    \"TKT-Team\" as team,\n    COUNT(*) FILTER (WHERE \"TKT-Status\" NOT IN ('Closed', 'Incident Resolved')) as open_tickets,\n    COUNT(*) FILTER (WHERE \"TKT-Status\" NOT IN ('Closed', 'Incident Resolved') \n                     AND \"TKT-Assigned To User\" LIKE '%PendingAssignment%') as unassigned,\n    COUNT(*) FILTER (WHERE \"TKT-Status\" IN ('Closed', 'Incident Resolved') \n                     AND \"TKT-Modified Time\" >= NOW() - INTERVAL '30 days') as closed_30d\n  FROM servicedesk.tickets\n  WHERE \"TKT-Team\" IS NOT NULL AND \"TKT-Team\" != ''\n  GROUP BY 1\n)\nSELECT \n  team as \"Team\",\n  open_tickets as \"Open\",\n  unassigned as \"Unassigned\",\n  CASE WHEN open_tickets > 0 THEN ROUND(100.0 * unassigned / open_tickets, 1) ELSE 0 END as \"Unassigned %\"\nFROM team_stats\nWHERE open_tickets > 0\nORDER BY open_tickets DESC\nLIMIT 10;",
