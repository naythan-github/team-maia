    "timezone": "browser",
    "schemaVersion": 38,
    "version": 0,
    "refresh": "1h",
    "time": {
      "from": "now-90d",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": [
        "1h",
        "6h",
        "1d"
      ]
