      {
        "id": 7,
        "type": "table",
        "title": "Top 10 Repetitive Alert Titles",
        "description": "Most common alert patterns - prime automation candidates",
        "gridPos": {
          "h": 10,
          "w": 12,
          "x": 12,
          "y": 20
        },
        "targets": [
          {
            "datasource": {
              "type": "postgres",
              "uid": "${datasource}"
            },
            "format": "table",
            "rawSql": "SELECT \n  \"TKT-Title\" as \"Alert Title\",\n  COUNT(*) as \"Occurrences\",\n  ROUND(COUNT(*) * 1.0 / 104 * 365, 0) as \"Projected Annual\",\n  '$' || ROUND(COUNT(*) * 1.0 / 104 * 365 * 0.5 * 80 / 1000, 1) || 'K' as \"Est. Savings (30min avg)\"\nFROM servicedesk.tickets\nWHERE \"TKT-Created Time\" >= '2025-07-01' AND \"TKT-Created Time\" <= '2025-10-13'\nAND \"TKT-Category\" = 'Alert'\nGROUP BY 1\nORDER BY 2 DESC\nLIMIT 10;",
            "refId": "A"
          }
        ],
