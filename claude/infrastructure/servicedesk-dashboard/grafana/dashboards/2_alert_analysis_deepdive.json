        "targets": [
          {
            "datasource": {
              "type": "postgres",
              "uid": "${datasource}"
            },
            "format": "table",
            "rawSql": "SELECT \n  EXTRACT(DOW FROM \"TKT-Created Time\") as \"Day\",\n  EXTRACT(HOUR FROM \"TKT-Created Time\") as \"Hour\",\n  COUNT(*) as \"Alert Count\"\nFROM servicedesk.tickets\nWHERE \"TKT-Created Time\" >= '2025-07-01' AND \"TKT-Created Time\" <= '2025-10-13'\nAND \"TKT-Category\" = 'Alert'\nGROUP BY 1, 2\nORDER BY 1, 2;",
