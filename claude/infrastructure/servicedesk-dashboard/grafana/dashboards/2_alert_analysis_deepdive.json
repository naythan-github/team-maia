      {
        "id": 8,
        "type": "piechart",
        "title": "Alert Distribution by Pattern Type",
        "description": "Breakdown of all alert categories",
        "gridPos": {
          "h": 10,
          "w": 12,
          "x": 0,
          "y": 30
        },
        "targets": [
          {
            "datasource": {
              "type": "postgres",
              "uid": "${datasource}"
            },
            "format": "table",
            "rawSql": "SELECT \n  CASE \n    WHEN \"TKT-Title\" ILIKE '%motion%' OR \"TKT-Title\" ILIKE '%sensor%' THEN 'Motion/Sensor'\n    WHEN \"TKT-Title\" ILIKE '%patch%' OR \"TKT-Title\" ILIKE '%update fail%' THEN 'Patch Failures'\n    WHEN \"TKT-Title\" ILIKE '%vpn%' OR \"TKT-Title\" ILIKE '%network%' THEN 'Network/VPN'\n    WHEN \"TKT-Title\" ILIKE '%azure%resource%' OR \"TKT-Title\" ILIKE '%azure health%' THEN 'Azure Resource'\n    WHEN \"TKT-Title\" ILIKE '%ssl%' OR \"TKT-Title\" ILIKE '%certificate%expir%' THEN 'SSL/Certificate'\n    ELSE 'Other Alerts'\n  END as \"Alert Type\",\n  COUNT(*) as \"Count\"\nFROM servicedesk.tickets\nWHERE \"TKT-Created Time\" >= '2025-07-01' AND \"TKT-Created Time\" <= '2025-10-13'\nAND \"TKT-Category\" = 'Alert'\nGROUP BY 1\nORDER BY 2 DESC;",
            "refId": "A"
          }
        ],
